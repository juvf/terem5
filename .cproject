							<option id="iar.arm.toolchain.option.misra-c:1998.rul13:.1509228206.112473916" name="13. [advisory] The basic types of 'char', 'int', 'short', 'long', 'float', and 'double' should not be used, but specific-length equivalents should be 'typedef' 'd for the specific compiler, and these type names used in the code" superClass="iar.arm.toolchain.option.misra-c:1998.rul13:.1509228206"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul14:.594937474.1174234025" name="14. [required] type 'char' shall always be declared as 'unsigned' 'char' or 'signed' 'char'" superClass="iar.arm.toolchain.option.misra-c:1998.rul14:.594937474"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul15:.472257694.557736980" name="15. [advisory] Floating point implementations should comply with a defined floating point standard" superClass="iar.arm.toolchain.option.misra-c:1998.rul15:.472257694"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul16:.1845566408.1705468246" name="16. [required] The underlying bit representation of floating point numbers shall not be used in any way by the programmer" superClass="iar.arm.toolchain.option.misra-c:1998.rul16:.1845566408"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul17:.1826127160.1412955074" name="17. [required] 'typedef' names shall not be reused" superClass="iar.arm.toolchain.option.misra-c:1998.rul17:.1826127160"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul18:.1906753406.1780254972" name="18. [advisory] Numeric constants should be suffixed to indicate type, where an appropriate suffix is available" superClass="iar.arm.toolchain.option.misra-c:1998.rul18:.1906753406"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul19:.1768089491.1471509289" name="19. [required] Octal constants (other than zero) shall not be used" superClass="iar.arm.toolchain.option.misra-c:1998.rul19:.1768089491"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul20:.1100559508.756105060" name="20. [required] All object and function identifiers shall be declared before use" superClass="iar.arm.toolchain.option.misra-c:1998.rul20:.1100559508"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul21:.425399335.198875424" name="21. [required] Identifiers in an inner scope shall not use the same name as an identifier in an outer scope, and therefore hide the identifier" superClass="iar.arm.toolchain.option.misra-c:1998.rul21:.425399335"/>
