							<option id="iar.arm.toolchain.option.misra-c:1998.rul23:.72919784.50145580" name="23. [advisory] All declarations at file scope should be static where possible" superClass="iar.arm.toolchain.option.misra-c:1998.rul23:.72919784"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul24:.350992916.455482214" name="24. [required] Identifiers shall not simultaneously have both internal and external linkage in the same translation unit" superClass="iar.arm.toolchain.option.misra-c:1998.rul24:.350992916"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul25:.1178861202.1467048708" name="25. [required] An identifier with external linkage shall have exactly one external definition" superClass="iar.arm.toolchain.option.misra-c:1998.rul25:.1178861202"/>
