							<option id="iar.arm.toolchain.option.misra-c:1998.rul17:.1826127160" name="17. [required] 'typedef' names shall not be reused" superClass="iar.arm.toolchain.option.misra-c:1998.rul17:"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul18:.1906753406" name="18. [advisory] Numeric constants should be suffixed to indicate type, where an appropriate suffix is available" superClass="iar.arm.toolchain.option.misra-c:1998.rul18:"/>
							<option id="iar.arm.toolchain.option.misra-c:1998.rul19:.1768089491" name="19. [required] Octal constants (other than zero) shall not be used" superClass="iar.arm.toolchain.option.misra-c:1998.rul19:"/>
